{
  "semi": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "proseWrap": "always",
  "importOrder": [
    "^packages/(.*)$",
    "^src/data/(.*)$",
    "^src/common/(.*)$",
    "^src/features/(.*)$",
    "^src/(.*)$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": false
}
